    "@tailwindcss/typography": "^0.3.1",
    "alpinejs": "^2.6.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "html-minifier": "^4.0.0",
    "js-yaml": "^3.14.0",
    "luxon": "^1.25.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "prismjs": "^1.21.0",
    "tailwindcss": "^2.0.2"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^0.6.0",
    "autoprefixer": "^10.1.0",
